            animation: sparkle 2s ease-in-out infinite reverse;
        }

        @keyframes sparkle {
            0%, 100% { transform: scale(1) rotate(0deg); }
            50% { transform: scale(1.2) rotate(10deg); }
        }

        /* Romantic title */
        .romantic-title {
            font-family: 'Dancing Script', cursive;
